
public class test3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		/*
		 * for(int i=1; i<101; i++) { System.out.print(i + " "); if((i%10)==0)
		 * System.out.println("\n"); // System.out.println("i : " + i + "\n");
		 * 
		 * }
		 */
		
		int i=0;
		while(true)
		{
			++i;
			System.out.print(i);
			if((i%100)==0)
				System.out.println("\n");
			if(i==100)
				break;
		}
	}

}
