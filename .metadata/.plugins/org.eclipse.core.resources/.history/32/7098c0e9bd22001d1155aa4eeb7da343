

public class Ex08_printf_format {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//system.out.println();
		System.out.println(); //출력하고 줄바꿈 (엔터)
		System.out.print("A");
		System.out.print("B");
		System.out.print("C");
		System.out.println();
		System.out.print("D");
		System.out.println();
		int num = 100;
		System.out.println(num);
		
		System.out.println("num 값은 " + num + " 입니다.");
		
	}

}
