package kr.or.kosa;

public class Book {
	String name;
	int dollor;
	
	
	Book(String name, int dollor){
		this.name = name;
		this.dollor = dollor;
	}
	
	public void print() {
		bookName();
		bookDollor();
	}
	public void bookName() {
		System.out.println("책이름 : " + this.name);
	}
	public void bookDollor() {
		System.out.println("가격 : " + this.dollor);
	}
}
