
public class Ex11_Statement {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int i =0;
		while(i >= 10)
		{
			//반드시
			//증가감을 명시
			i--;
			System.out.println("i : " + i);
		}
		
		int sum = 0;
		int j = 1;
		while(j <= 100)
		{
			sum += j;
			j++;
		}
		System.out.println("sum : " + sum);
		
		//while 구구단을 출력하세요.
	}

}
